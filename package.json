{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\"}' server/index.ts",
    "server": "ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\"}' server/api.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .next/production-server/server/index.js",
    "type-check": "tsc"
  },
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.3.1",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "apollo-server": "^2.25.4",
    "compression": "^1.7.4",
    "cookie": "^0.3.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "next": "^8.0.4",
    "next-routes": "^1.4.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.4",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2",
    "ts-node": "^8.3.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.2",
    "@types/next": "^7.0.6",
    "@types/react": "^16.4.16",
    "@types/react-dom": "16.0.11",
    "typescript": "^3.5.2"
  },
  "license": "ISC"
}
